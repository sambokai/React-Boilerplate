{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "plugins": ["jest"],
  "rules": {
    "react/jsx-one-expression-per-line": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/jest.setup.js", "**/*.test.{js,jsx}", "**/webpack.*.js"]}]
  }
}
